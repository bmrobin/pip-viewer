{
  "name": "pip-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "filewatcher-webpack-plugin": "1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.4",
    "jest-junit": "^3.1.0",
    "less": "3.0.4",
    "less-loader": "^4.0.5",
    "nodemon": "1.17.5",
    "npm-run-all": "4.1.3",
    "prettier": "^1.8.2",
    "react-dev-utils": "^4.1.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.5",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "yarn": "^1.1.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "test": "jest",
    "dev": "webpack-dev-server"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": "usage",
          "targets": {
            "node": 8,
            "browsers": [
              "last 2 Chrome versions"
            ],
            "modules": false
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "add-react-displayname",
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "camelcase": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/extensions": 0,
      "import/prefer-default-export": 1,
      "class-methods-use-this": 0,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/jsx-filename-extension": 0,
      "react/no-multi-comp": 0,
      "react/react-in-jsx-scope": 2,
      "react/no-array-index-key": 0,
      "prettier/prettier": "error",
      "jsx-a11y/label-has-for": 0
    },
    "plugins": [
      "prettier",
      "react"
    ]
  },
  "jest": {
    "resetMocks": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/scripts/testSetup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "testResultsProcessor": "jest-junit",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  }
}
